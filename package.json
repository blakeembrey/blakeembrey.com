{
  "name": "blakeembrey.com",
  "description": "Personal blog of Blake Embrey",
  "private": true,
  "scripts": {
    "pretty": "prettier --single-quote --write \"{pages,loaders,wrappers,utils}/**/*.{js,jsx}\" \"gatsby-{node,browser}.js\" html.js",
    "clean": "rimraf public",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && NODE_ENV=production gatsby build --prefix-links && gh-pages -d public",
    "dev": "gatsby develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blakeembrey/blakeembrey.com.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react-free-style": "^4.0.0",
    "react-ga": "^2.1.2",
    "react-helmet": "^5.0.2"
  },
  "devDependencies": {
    "feed": "^1.0.2",
    "front-matter": "^2.1.2",
    "gatsby": "^0.12.41",
    "gh-pages": "^0.12.0",
    "highlighter": "^0.1.0",
    "markdown-it": "^8.3.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attrs": "^0.8.0",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "moment": "^2.6.0",
    "postcss": "^5.2.16",
    "postcss-js": "^0.3.0",
    "prettier": "^0.22.0",
    "rimraf": "^2.6.1"
  }
}
